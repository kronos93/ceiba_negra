.container-icons
	text-align center
	background #fff
	h4
		display inline-block
		vertical-align middle
	i
		vertical-align middle
	&.error
		i
			color:#F27474
	&.warning
		i
			color:#F8BB86
	&.success
		i
			color:#A5DC86
