header
	absolute:left top
	width:100%
	z-index:999
	+above(3)
		position:initial
.navbar-default
	border-bottom:5px solid $grayColor
.brand-ceiba
	background:#B88D2C
	absolute:left 50%
	top:-1px
	transform:translateX(-50%)
	padding:5px 50px
	box-shadow:0px 0px 3px #000
	border-radius: 0px 0px 3px 3px
	img
		display:block
		margin:0 auto
		padding:5px
		box-sizing:border-box
		max-width:70px
	figure
		position:relative
		figcaption
			text-transform:uppercase
			font-style:none
			font-size:20px
			font-family:'panama-light'
			text-align:center
			color:#fff
	&:hover,&:focus
		text-decoration:none
		border:none
	&:after
		content:''
		height:1px
		position:absolute
		top:73px
		left:10px
		border-right:60px solid #fff
	&:before
		content:''
		height:1px
		position:absolute
		top:73px
		right:10px
		border-right:60px solid #fff
			
