html
	height:100%
body
	height: 100%
	width:100%
	line-height: 1.3
	letter-spacing: .5px
	font-size: 13px
	background:#f6f5fa
strong 
	font-weight: 700
small
	font-size: 70%
iframe
	width:100%
h5.title
	background:$primaryColor
	height:35px
	text-transform:uppercase
	line-height:35px
	padding:0px 10px 
	box-sizing:border-box
	border-radius:5px
	color:#fff
	font-weight:400
caption,tr,td
	text-align: left!important
	font-weight: normal!important
	vertical-align: middle!important
p
	margin:0
span.peso
	&:before
		content:'$'
	&:after
		content:'.00'
.form-group
	label.required:before
		content:'*'
		absolute:top 0px left 6px
		color:$primaryColor
		font-size:18px
//estilos generales del main
main.wrap-main
	padding:70px 0px 30px
	.dataTables_wrapper
		padding:10px 0px
	+from-width(600px)
		padding:0px 0px 30px
	+from-width(1200px)
		padding:20px 0px 30px